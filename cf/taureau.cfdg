startshape CUBIC

rule CUBIC 10 {
	CUBIC { x 3 s 0.2 r 60}
	CUBIC { x 4 s 0.6 r -20}
	CUBIC { x 3 s 0.9 r 10}
	CUBIC { x 5 s 0.5 r -15}
	CUBIC { y 3 s 0.5 r 90}
}

rule CUBIC  {
	TWOPIXELS {r 45}
	TWOPIXELS {y 5}
}

rule CUBIC  {
	TWOPIXELS {r 60}
	TWOPIXELS {x 10}
}

rule TWOPIXELS  {
	SQUARE { x 1 }
	SQUARE { y 1 }
}